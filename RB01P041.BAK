#include<stdio.h>
#include<conio.h>
void main()
  {
     int i,number,fact=2;
     clrscr();
     printf("Enter any number :");
     scanf("%d",&number);
     while(number!=1)
	{
	   if(number%fact==0)
	      {
		 printf("%d",fact);
		 number=number/fact;
		 if(number!=1)
		    {
		       printf("*");
		    }
	      }
	   else
	      {
		 fact++;
	      }
	}
     getch();
  }
