#include<stdio.h>
#include<conio.h>
void main()
  {
     int j,i=1,range,count=0,number=2,prime=0;
     clrscr();
     printf("Enter the range : ");
     scanf("%d",&range);
     printf("\n-------- Prime numbers are --------\n");
     while(prime!=range)
	{
	   for(i=2;i<=number/2;i++)
	      {
		 count=0;
		 if(number%i==0)
		    {
		       count++;
		       break;
		    }
	      }
	   if(count==0)
	      {
		 prime++;
		 printf("%d\t",number);

	      }
	   number++;
	}
     getch();
  }

